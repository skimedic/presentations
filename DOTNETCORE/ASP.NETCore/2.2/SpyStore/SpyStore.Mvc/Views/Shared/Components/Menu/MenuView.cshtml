@model IEnumerable<SpyStore.Models.Entities.Category>
<li><a asp-controller="Products" asp-action="Featured" class="nav-link">Featured</a></li>
@foreach (var item in Model)
{
    <li><a asp-controller="Products" asp-action="ProductList" asp-route-id="@item.Id" class="nav-link">@item.CategoryName</a></li>
}

